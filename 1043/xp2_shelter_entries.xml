<ZT2Strings>
      <LOC_STRING _locID="zoopedia_kelpbed:entry">Kelpbed</LOC_STRING>
      <LOC_STRING _locID="zoopedia_kelpbed:text">
			<cell pady="0" width="1000"/>
			<p/>
			<cell width="260"/>
			<cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
				<cell width="600" padx="20" pady="20">
					<color r="255" g="255" b="255">
						<img src="entities/objects/shelters/KelpBed_Shelter/KelpBed_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
						<p/>Zeeotters rusten graat uit in kelpbedden. Ze draaien zichzelf in een kelpbed voordat ze gaan slapen om te voorkomen dat ze afdrijven!
					</color>
				</cell>
			</cell>
		</LOC_STRING>
      <LOC_STRING _locID="zoopedia_desertisland:entry">Woestijneiland</LOC_STRING>
