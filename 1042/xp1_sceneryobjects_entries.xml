            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <br/>
                  <img src="entities\objects\scenery\Statue_20000Guest\Statue_20000Guest_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <br/>
