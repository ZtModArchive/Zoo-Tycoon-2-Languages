   <!--Zoopedia entries-->
   <zoopedia_brickwindow>
	      <entry>벽돌 창문 울타리</entry>
	      <text>
	         <cell pady="0" width="1000"/>
	         <p/>
	         <cell width="260"/>
	         <cell width="640" bgimg="ui/zoopedia/topCellBg.dds">
	            <cell width="600" padx="20" pady="20">
	               <color r="255" g="255" b="255">
	                  <img src="entities\objects\fences\brickwindow_df\brickwindow_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
	                  <p/>벽돌 창문 울타리는 단단해서 동물들이 울타리 밖으로 나가지 못 할 뿐만 아니라 창문을 통해 관람객들이 울타리 안의 동물을 잘 볼 수 있습니다.
	                  <p/>단단하면서도 관람객들에게 인기 있는 울타리를 만들려면 벽돌 울타리와 벽돌 창문 울타리를 섞어서 만드세요.
	               </color>
	            </cell>
	         </cell>
	      </text>
   </zoopedia_brickwindow>
   <zoopedia_stonewindow>
		      <entry>돌 창문 울타리</entry>
		      <text>
		         <cell pady="0" width="1000"/>
