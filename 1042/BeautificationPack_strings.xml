                  <img src="entities\objects\fences\grasslandfence_df\grasslandfence_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities\objects\fences\savannahfence_df\savannahfence_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities\objects\fences\scrubfence_df\scrubfence_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities\objects\fences\temperatefence_df\temperatefence_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>                   
                  <img src="entities\objects\fences\tropicalfence_df\tropicalfence_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
