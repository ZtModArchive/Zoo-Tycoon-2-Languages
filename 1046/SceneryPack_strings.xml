   <!--Zoopedia entries-->
   <zoopedia_brickwindow>
	      <entry>Muro de tijolo com janelas</entry>
	      <text>
	         <cell pady="0" width="1000"/>
	         <p/>
	         <cell width="260"/>
	         <cell width="640" bgimg="ui/zoopedia/topCellBg.dds">
	            <cell width="600" padx="20" pady="20">
	               <color r="255" g="255" b="255">
	                  <img src="entities\objects\fences\brickwindow_df\brickwindow_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
	                  <p/>O muro de tijolo com janelas permite que os visitantes tenham uma excelente visão dos animais, ao mesmo tempo que os protege contra possíveis fugas.
	                  <p/>Use o muro de tijolo com janelas associado ao muro de tijolo normal para obter um cercado durável e atraente.
	               </color>
	            </cell>
	         </cell>
	      </text>
   </zoopedia_brickwindow>
