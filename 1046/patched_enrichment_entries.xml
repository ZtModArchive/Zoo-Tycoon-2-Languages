                  <p/>
                  <img src="entities/objects/food/SubstratePile/SubstratePile_Apples_icon.dds" sx="0" sy="0" sw="64" sh="64"/> <img src="entities/objects/food/SubstratePile/SubstratePile_Bamboo_icon.dds" sx="0" sy="0" sw="64" sh="64"/> <img src="entities/objects/food/SubstratePile/SubstratePile_Bananas_icon.dds" sx="0" sy="0" sw="64" sh="64"/> <img src="entities/objects/food/SubstratePile/SubstratePile_Branches_icon.dds" sx="0" sy="0" sw="64" sh="64"/> <img src="entities/objects/food/SubstratePile/SubstratePile_Hay_icon.dds" sx="0" sy="0" sw="64" sh="64"/> <img src="entities/objects/food/SubstratePile/SubstratePile_Insects_icon.dds" sx="0" sy="0" sw="64" sh="64"/> <img src="entities/objects/food/SubstratePile/SubstratePile_Lettuce_icon.dds" sx="0" sy="0" sw="64" sh="64"/> <img src="entities/objects/food/SubstratePile/SubstratePile_Grass_icon.dds" sx="0" sy="0" sw="64" sh="64"/> <img src="entities/objects/food/SubstratePile_df/SubstratePileBerries_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>                  
                  <p/>
