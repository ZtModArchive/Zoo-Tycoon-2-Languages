<ZT2Strings>
      <LOC_STRING _locID="zoopedia_benthicrocks:entry">Rochas bênticas</LOC_STRING>
      <LOC_STRING _locID="zoopedia_benthicrocks:text">
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/rocks/benthic/Rock_Benthic_Small_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities/objects/rocks/benthic/Rock_Benthic_Medium_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities/objects/rocks/benthic/Rock_Benthic_Large_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>Use rochas bênticas em tanques com animais marinhos bênticos como o tubarão-baleia ou o tubarão-duende. Você também pode usar essas rochas para dar um toque marinho às áreas públicas.
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_reefrocks:entry">Rochas de recife</LOC_STRING>
