   <!--Zoopedia entries-->
   <zoopedia_brickwindow>
	      <entry>Ziegelmauer mit Fenstern</entry>
	      <text>
	         <cell pady="0" width="1000"/>
	         <p/>
	         <cell width="260"/>
	         <cell width="640" bgimg="ui/zoopedia/topCellBg.dds">
	            <cell width="600" padx="20" pady="20">
	               <color r="255" g="255" b="255">
	                  <img src="entities\objects\fences\brickwindow_df\brickwindow_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
	                  <p/>Die Ziegelmauer mit Fenstern bietet Besuchern einen hervorragenden Blick auf die Tiere und Sie können sicher sein, dass die Tiere nicht entlaufen.
	                  <p/>Verwenden Sie die Ziegelmauer mit Fenstern mit der normalen Ziegelmauer für ein attraktives und beständiges Gehege.
	               </color>
	            </cell>
	         </cell>
	      </text>
   </zoopedia_brickwindow>
   <zoopedia_stonewindow>
		      <entry>Steinmauer mit Fenstern</entry>
		      <text>
		         <cell pady="0" width="1000"/>
		         <p/>
		         <cell width="260"/>
		         <cell width="640" bgimg="ui/zoopedia/topCellBg.dds">
		            <cell width="600" padx="20" pady="20">
		               <color r="255" g="255" b="255">
		                  <img src="entities\objects\fences\stonewindow_df\stonewindow_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
		                  <p/>Die Steinmauer mit Fenstern bietet Besuchern einen hervorragenden Blick auf die Tiere und Sie können sicher sein, dass die Tiere nicht entlaufen.
		                  <p/>Verwenden Sie die Steinmauer mit Fenstern mit der normalen Steinmauer für ein attraktives und beständiges Gehege.
		               </color>
		            </cell>
		         </cell>
		      </text>
   </zoopedia_stonewindow>
   <zoopedia_junglewindow>
		      <entry>Dschungelzaun mit Fenstern</entry>
		      <text>
		         <cell pady="0" width="1000"/>
		         <p/>
		         <cell width="260"/>
		         <cell width="640" bgimg="ui/zoopedia/topCellBg.dds">
		            <cell width="600" padx="20" pady="20">
		               <color r="255" g="255" b="255">
		                  <img src="entities\objects\fences\junglewindow_jt\junglewindow_jt_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
		                  <p/>Der Dschungelzaun mit Fenstern bietet Besuchern einen hervorragenden Blick auf die Tiere und Sie können sicher sein, dass die Tiere nicht entlaufen.
		                  <p/>Zäune mit einem Dschungelthema eignen sich für Gehege mit Tieren des Dschungels und des Regenwalds, beispielsweise Tiger, Gorillas und Schimpansen. Themenobjekte sind bei den Besuchern beliebt.
		               </color>
		            </cell>
		         </cell>
		      </text>
   </zoopedia_junglewindow> 
   <zoopedia_tortoisetrampoline>
      <entry>Trampolin, Schildkröten-</entry>
      <text>
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
               <img src="entities/objects/scenery/Playground_TortoiseTrampoline_df/Playground_TortoiseTrampoline_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>               
                  <p/>
                  <p/>Kinder lieben es, auf den Rücken der bunten, aufblasbaren Schildkröte zu klettern und auf diesem Trampolin hoch in die Luft zu springen.
               </color>
            </cell>
         </cell>
      </text>
   </zoopedia_tortoisetrampoline>
   <zoopedia_fountainsmall>
      <entry>Brunnen, Kleiner</entry>
      <text>
         <cell pady="0" width="1000"/>
