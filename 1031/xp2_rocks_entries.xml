<ZT2Strings>
      <LOC_STRING _locID="zoopedia_benthicrocks:entry">Meeresbodenfelsen</LOC_STRING>
      <LOC_STRING _locID="zoopedia_benthicrocks:text">
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/rocks/benthic/Rock_Benthic_Small_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities/objects/rocks/benthic/Rock_Benthic_Medium_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities/objects/rocks/benthic/Rock_Benthic_Large_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>Platzieren Sie Meeresbodenfelsen in Becken mit Meerestieren wie dem Walhai oder dem Koboldhai, die sich in der Nähe des Meeresbodens aufhalten. Sie können Meeresbodenfelsen auch in Besucherbereichen platzieren, um eine maritime Atmosphäre zu schaffen.
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_reefrocks:entry">Rifffelsen</LOC_STRING>
      <LOC_STRING _locID="zoopedia_reefrocks:text">
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/rocks/reef/Rock_Reef_Small_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities/objects/rocks/reef/Rock_Reef_Medium_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities/objects/rocks/reef/Rock_Reef_Large_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>Platzieren Sie Rifffelsen in Becken, in denen Meerestiere wie der Mantarochen oder der Schwarzspitzen-Riffhai leben, die sich in Rifffelsen aufhalten. Sie können Rifffelsen auch in Besucherbereichen platzieren, um eine maritime Atmosphäre zu schaffen.
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_coastalrocks:entry">Küstenfelsen</LOC_STRING>
      <LOC_STRING _locID="zoopedia_coastalrocks:text">
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/rocks/coastal/Rock_Coastal_Small_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities/objects/rocks/coastal/Rock_Coastal_Medium_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities/objects/rocks/coastal/Rock_Coastal_Large_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>Platzieren Sie Küstenfelsen in Gehegen mit Tieren, die sich an Küsten aufhalten, wie dem Kalifornischen Seelöwen oder dem Felsenpinguin. Sie können Küstenfelsen auch in Besucherbereichen platzieren, um eine maritime Atmosphäre zu schaffen.
               </color>
            </cell>
         </cell>
      </LOC_STRING>

</ZT2Strings>
