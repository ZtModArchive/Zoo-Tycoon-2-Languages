<ZT2Strings>
      <LOC_STRING _locID="ZTPhotoMode" version="9"/>
      <LOC_STRING _locID="ZTPhotoMode:go_to_cursor_mode" version="9">
         <font name="arial" size="12" shadowx="0" shadowy="0" shadowa="60">
            <color r="255" g="255" b="255" a="100">
               <b>Verwenden Sie die Pfeiltasten zur Fortbewegung und die rechte Maustaste zum Anzeigen der Maus- und Menüschaltflächen.</b>
            </color>
         </font>
      </LOC_STRING>
      <LOC_STRING _locID="ZTPhotoMode:go_to_photo_mode" version="9">
         <font name="arial" size="12" shadowx="0" shadowy="0" shadowa="60">
            <color r="255" g="255" b="255" a="100">
               <b>Klicken Sie hier mit der rechten Maustaste, um in den Fotomodus zu wechseln.</b>
            </color>
         </font>
      </LOC_STRING>
      <LOC_STRING _locID="ZTPhotoMode:go_to_game_mode" version="9">
         <font name="arial" size="12" shadowx="0" shadowy="0" shadowa="60">
            <color r="255" g="255" b="255" a="100">
               <b>Klicken Sie mit der rechten Maustaste, um die Menüschaltflächen auszublenden und zurück ins Spiel zu gelangen.</b>
            </color>
         </font>
      </LOC_STRING>
</ZT2Strings>
