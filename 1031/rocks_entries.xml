                  <a href="zoopedia:zoopedia_yellowcedar:entry">
                     <u>
                        <color r="255" g="231" b="135">Scheinzypresse </color>
                     </u>
                  </a>
                  
                  
                   
                und der 
		     
                  
                  
                  <a href="zoopedia:zoopedia_himalayanbirch:entry">
                     <u>
                        <color r="255" g="231" b="135">Himalaya-Birke </color>
                     </u>
                  </a>
                  
                  
                                  
                 und 
		     
                  
                  
                  <a href="zoopedia:zoopedia_himalayanpine:entry">
                     <u>
                        <color r="255" g="231" b="135">der Himalaya-Kiefer </color>
                     </u>
                  </a>
                  
                  
                                   
		 .
				
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
