      <LOC_STRING _locID="zoopedia_fooddish:entry" version="9">Futternapf</LOC_STRING>
      <LOC_STRING _locID="zoopedia_fooddish:text" version="9">
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
