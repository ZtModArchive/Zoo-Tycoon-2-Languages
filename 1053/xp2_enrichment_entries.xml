<ZT2Strings>
      <LOC_STRING _locID="zoopedia_bubbleemitter:entry">Musselbubbelbad</LOC_STRING>
      <LOC_STRING _locID="zoopedia_bubbleemitter:text">
		<cell pady="0" width="1000"/>
		<p/>
		<cell width="260"/>
		<cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
		 <cell width="600" padx="20" pady="20">
		  <color r="255" g="255" b="255">
			<img src="entities/objects/enrichment/BubbleEmitter/BubbleEmitter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
			<p/>Marina djur, som djävulsrockan, tycker om att simma i bubbelströmmen som skapas av den här konstgjorda jättemusslan.
		  </color>
		 </cell>
		</cell>
	  </LOC_STRING>
      <LOC_STRING _locID="zoopedia_jarwithfish:entry">Burk med fisk</LOC_STRING>
