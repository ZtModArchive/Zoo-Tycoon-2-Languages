<ZT2Strings>
      <LOC_STRING _locID="zoopedia_kelpbed:entry">Kelpbädd</LOC_STRING>
      <LOC_STRING _locID="zoopedia_kelpbed:text">
			<cell pady="0" width="1000"/>
			<p/>
			<cell width="260"/>
			<cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
				<cell width="600" padx="20" pady="20">
					<color r="255" g="255" b="255">
						<img src="entities/objects/shelters/KelpBed_Shelter/KelpBed_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
						<p/>Havsuttrar föredrar att sova i kelpbäddar. De virar in sig i kelpen innan de somnar för att undvika att driva iväg!
					</color>
				</cell>
			</cell>
		</LOC_STRING>
      <LOC_STRING _locID="zoopedia_desertisland:entry">Öde ö</LOC_STRING>
      <LOC_STRING _locID="zoopedia_desertisland:text">
			<cell pady="0" width="1000"/>
			<p/>
			<cell width="260"/>
			<cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
				<cell width="600" padx="20" pady="20">
					<color r="255" g="255" b="255">
						<img src="entities/objects/shelters/DesertIsland/DesertIsland_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
						<p/>Den öde ön är inte bara en dekoration i bassängen, där kan även soppsköldpaddan lägga sina ägg och vila.
					</color>
				</cell>
			</cell>
		</LOC_STRING>
      <LOC_STRING _locID="zoopedia_rockisland:entry">Klippig ö</LOC_STRING>
