   <!--Zoopedia entries-->
   <zoopedia_brickwindow>
	      <entry>Brick Window Fence</entry>
	      <text>
	         <cell pady="0" width="1000"/>
	         <p/>
	         <cell width="260"/>
	         <cell width="640" bgimg="ui/zoopedia/topCellBg.dds">
	            <cell width="600" padx="20" pady="20">
	               <color r="255" g="255" b="255">
	                  <img src="entities\objects\fences\brickwindow_df\brickwindow_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
	                  <p/>The brick window fence provides your guests with an excellent view of the animals while providing security against animal escapes at the same time.
	                  <p/>Use the brick window fence with the regular brick fence for an attractive and durable exhibit.
	               </color>
	            </cell>
	         </cell>
	      </text>
   </zoopedia_brickwindow>
   <zoopedia_stonewindow>
		      <entry>Stone Window Fence</entry>
		      <text>
		         <cell pady="0" width="1000"/>
		         <p/>
		         <cell width="260"/>
		         <cell width="640" bgimg="ui/zoopedia/topCellBg.dds">
		            <cell width="600" padx="20" pady="20">
		               <color r="255" g="255" b="255">
		                  <img src="entities\objects\fences\stonewindow_df\stonewindow_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
		                  <p/>The stone window fence provides your guests with an excellent view of the animals while providing security against animal escapes at the same time.
