   <zoopedia_junglewindow>
		      <entry>Jungle Window Fence</entry>
		      <text>
		         <cell pady="0" width="1000"/>
		         <p/>
		         <cell width="260"/>
		         <cell width="640" bgimg="ui/zoopedia/topCellBg.dds">
		            <cell width="600" padx="20" pady="20">
		               <color r="255" g="255" b="255">
		                  <img src="entities\objects\fences\junglewindow_jt\junglewindow_jt_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
		                  <p/>The jungle window fence provides your guests with an excellent view of the animals while providing security against animal escapes at the same time.
		                  <p/>Fencing that reflects a jungle theme is best used when building exhibits for jungle and rainforest animals, including the tiger, gorilla, and chimpanzee. Guests appreciate themed objects.
		               </color>
		            </cell>
		         </cell>
		      </text>
   </zoopedia_junglewindow> 
   <zoopedia_tortoisetrampoline>
      <entry>Trampoline, Tortoise</entry>
      <text>
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
               <img src="entities/objects/scenery/Playground_TortoiseTrampoline_df/Playground_TortoiseTrampoline_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>               
                  <p/>
                  <p/>Kids love to climb up on the back of this colorful inflatable tortoise and bounce high into the air on the trampoline.
               </color>
            </cell>
         </cell>
      </text>
   </zoopedia_tortoisetrampoline>
