<ZT2Strings>
      <LOC_STRING _locID="zoopedia_beaverlodge:entry"  >Beaver Lodge</LOC_STRING>
      <LOC_STRING _locID="zoopedia_beaverlodge:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  
                  
                  
                  
                  
                  
                  
            Beavers construct the beaver lodge from sticks, twigs and branches placed in their exhibit.  
                  
                  
                  
                  
                  
                  
                  
                  <a href="zoopedia:zoopedia_beaver:entry">
                     <u>
                        <color r="255" g="231" b="135">Beavers </color>
                     </u>
                  </a>
                  
                   enter the lodge by swimming into the entrance.
               
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_cubbingden:entry"  >Cubbing Den</LOC_STRING>
      <LOC_STRING _locID="zoopedia_cubbingden:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/shelters/CubbingDen_Shelter/CubbingDen_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>
                  
                  
                  
                  
                  
                  
                  A cubbing den is a warm, quiet enclosure where animals can rest with their offspring.	             
                  
                  
                  
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_elephanthouse:entry"  >Elephant House</LOC_STRING>
      <LOC_STRING _locID="zoopedia_elephanthouse:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/shelters/ElephantHouse_Shelter/ElephantHouse_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>
                  
                  
                  
                  
                  
                  
                  
                  
                  
                                                 
               This large, roomy concrete structure is designed to accommodate the 
               particularly large mass of the elephant.
				
               
               
               
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_nestbox:entry"  >Nest Box, Elevated</LOC_STRING>
      <LOC_STRING _locID="zoopedia_nestbox:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/shelters/ElevatedNestBox_Shelter/ElevatedNestBox_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>
                  
                  
                  
                  
                  
                  
                  
                  
                  
                  
            The elevated nest box is a small house-like shelter mounted on a tall pole, which 
               satisfies the needs of animals who like to nest above the ground.
				
               
               
               
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_giraffehouse:entry"  >Giraffe House</LOC_STRING>
      <LOC_STRING _locID="zoopedia_giraffehouse:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/shelters/GiraffeHouse_Shelter/GiraffeHouse_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>
                  
                  
                  
                  
                  
                  
                  
                  
                  
                  
               This tall building is designed to accommodate the extra height requirements of the 
               tall giraffe.
				
               
               
               
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_groundnest:entry"  >Ground Nest</LOC_STRING>
      <LOC_STRING _locID="zoopedia_groundnest:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  
                  
                  
                  
                  
                  
                  
                  
                  
                  
               A ground nest, which is really nothing more than a large pile of leaves, is a comfortable 
               resting place for many animals.
	       
                  
                  
                  
                  
                  
                  
                  
                  
                  
                  <p/>
                  
                  
                  
                  
                  
                  
                  
                  
                  
                  
	       For many animals, including the gorilla, building a nest is a highly instinctual activity.
				
               
               
               
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_treestump:entry"  >Tree Stump, Hollow</LOC_STRING>
      <LOC_STRING _locID="zoopedia_treestump:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/shelters/HollowTreeStump_Shelter/HollowTreeStump_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>
                  
                  
                  
                  
                  
                  
                  
                  
                  
                  
               Both the giant panda and the red panda consider a tall, hollow tree stump to be a very 
               natural and inviting form of shelter. Guests enjoy seeing animals use shelters that 
               more closely represent natural habitats.
				
               
               
               
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_concreteshelter:entry"  >Concrete Shelters</LOC_STRING>
      <LOC_STRING _locID="zoopedia_concreteshelter:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/shelters/SmallConcrete_Shelter/SmallConcrete_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities/objects/shelters/LargeConcrete_Shelter/LargeConcrete_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>
                  
                  
                  
                  
                  
                  
                  
                  
                  
                  
		The large concrete shelter is a man-made structure that provides a restful environment 
		and maximum protection for animals that prefer to sleep in an enclosure, including 
		tigers, cheetahs, and most bears. Up to 2 animals may sleep in the small concrete 
		shelter and up to 4 animals are accommodated by the large concrete shelter.
				
               
               
               
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_rockcave:entry"  >Rock Cave</LOC_STRING>
      <LOC_STRING _locID="zoopedia_rockcave:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/shelters/SmallRockCave_Shelter/SmallRockCave_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities/objects/shelters/LargeRockCave_Shelter/LargeRockCave_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>
                  
                  
                  
                  
                  
                  
                  
                  
                  
                  
               The large rock cave, which includes a hole in the roof to ensure adequate ventilation, 
               provides a natural habitat for animals, including the polar bear and cheetah, who prefer 
               a dark, enclosed resting environment. The small cave accommodates 2 animals, 
               and the large cave holds 4.
				
               
               
               
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_snowyrockcave:entry"  >Snow Cave</LOC_STRING>
      <LOC_STRING _locID="zoopedia_snowyrockcave:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/shelters/SmallSnowCave_Shelter/SmallSnowCave_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities/objects/shelters/LargeSnowCave_Shelter/LargeSnowCave_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>
                  
                  
                  
                  
                  
                  
                  
                   The large snow cave, which includes a hole in the roof to ensure adequate 
ventilation, provides a natural habitat for animals, including the 
polar bear, which is comfortable in frigid, dark, and enclosed resting environments. 
The small cave accommodates 2 animals, and the large cave holds 4.
				
               
               
               
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_stable:entry"  >Stables</LOC_STRING>
      <LOC_STRING _locID="zoopedia_stable:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/shelters/SmallStable_Shelter/SmallStable_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities/objects/shelters/LargeStable_Shelter/LargeStable_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>
                  
                  
                  
                  A stable is a wooden building that contains stalls bedded with straw, providing a comfortable enclosure for hoofed animals.  
               An exterior overhang provides shade and protection from the elements. The small stable has 2 
               stalls and the large stable has 4.
               
               
               
               
                  
                  
                  
                  <p/>
                  
                  
                  
                  Both the small and large stables provide a restful environment, privacy, and maximum protection for ungulates (animals with hooves).
  The large stable accommodates more animals than the small stable.
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_woodenshelter:entry"  >Wooden Shelters</LOC_STRING>
      <LOC_STRING _locID="zoopedia_woodenshelter:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/shelters/SmallWooden_Shelter/SmallWooden_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <img src="entities/objects/shelters/LargeWooden_Shelter/LargeWooden_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>
                  
                  
                  
                  
                  
                  
                  
                  
                  
                  
               This man-made structure provides protection for animals that prefer to sleep in an enclosure.
               Up to 2 animals may sleep in the small wooden shelter and up to 4 animals can sleep in the 
               large wooden shelter.
				
               
               
               
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_raisedsleepingplatform:entry"  >Raised Sleeping Platform</LOC_STRING>
      <LOC_STRING _locID="zoopedia_raisedsleepingplatform:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/shelters/RaisedSleepingPlatform_Shelter/RaisedSleepingPlatform_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>
                  
                  
                  
                  
                  
                  
                  
                  
                  
                  
               A raised platform with wooden ramps allows animals to walk up and sleep in a more protected position off 
               the ground.
				
               
               
               
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_shadestructure:entry"  >Shade Structure</LOC_STRING>
      <LOC_STRING _locID="zoopedia_shadestructure:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/shelters/ShadeStructure_Shelter/ShadeStructure_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>
                  
                  
                  
                  
                  
                  
                  
               This simple structure provides animals with a place to seek shelter from the sun.
               
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
      <LOC_STRING _locID="zoopedia_sleepingbasket:entry"  >Sleeping Basket</LOC_STRING>
      <LOC_STRING _locID="zoopedia_sleepingbasket:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities/objects/shelters/SleepingBasket_Shelter/SleepingBasket_Shelter_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>
                  
                  
                  
                  
                  
                  
                  
               A basket primarily used by primates. One large animal or two small ones can fit comfortably into the basket. 
               The basket can be made more restful if bedding material is placed in the exhibit.  
               Bedding material will deteriorate over time.
               
               
               
               
               
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
</ZT2Strings>
