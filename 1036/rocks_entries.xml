                  <a href="zoopedia:zoopedia_reindeerlichen:entry">
                     <u>
                        <color r="255" g="231" b="135">le lichen des rennes </color>
                     </u>
                  </a>
                  
                  
                                  
               et le  
		     
                  
                  
                  <a href="zoopedia:zoopedia_balsamfir:entry">
                     <u>
                        <color r="255" g="231" b="135">sapin baumier</color>
                     </u>
                  </a>
                  
                  
                                  
               , 
		     
                  
                  
                  <a href="zoopedia:zoopedia_blackspruce:entry">
                     <u>
                        <color r="255" g="231" b="135">l'épinette noire </color>
                     </u>
                  </a>
                  
                  
                                  
               et  
		     
                  
                  
                  <a href="zoopedia:zoopedia_tamarack:entry">
                     <u>
                        <color r="255" g="231" b="135">le mélèze laricin </color>
                     </u>
                  </a>
                  
                  
                                  
                .
				
               
               
               </color>
            </cell>
         </cell>
      </LOC_STRING>
