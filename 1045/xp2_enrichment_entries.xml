	<LOC_STRING _locID="zoopedia_sealpreydummy:entry"  >Wypchana kukła foki</LOC_STRING>
	<LOC_STRING _locID="zoopedia_sealpreydummy:text"  >
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <p/>
                  <img src="entities\objects\enrichment\Seal_Prey_Dummy\SealPreyDummy_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>Ta pływająca kukła została stworzona dla dużych morskich drapieżników, takich jak żarłacz biały lub orka. Zwierzęta uwielbiają bawić się tym obiektem.
               </color>
            </cell>
         </cell>
      </LOC_STRING>
</ZT2Strings>
