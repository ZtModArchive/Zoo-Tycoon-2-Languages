   <!--Zoopedia entries-->

   <zoopedia_brickwindow>
      <entry>Ażurowe ceglane ogrodzenie</entry>
      <text>
	         <cell pady="0" width="1000"/>
	         <p/>
	         <cell width="260"/>
	         <cell width="640" bgimg="ui/zoopedia/topCellBg.dds">
	            <cell width="600" padx="20" pady="20">
	               <color r="255" g="255" b="255">
	                  <img src="entities\objects\fences\brickwindow_df\brickwindow_df_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
	                  <p/>Ażurowe ceglane ogrodzenie zapewnia zwiedzającym doskonałą widoczność, a jednocześnie skutecznie zapobiega ucieczkom zwierząt.
	                  <p/>Użyj ażurowego ceglanego ogrodzenia razem ze zwykłym murem z cegły, aby stworzyć atrakcyjną ekspozycję.
	               </color>
	            </cell>
	         </cell>
	      </text>
   </zoopedia_brickwindow>

   <zoopedia_stonewindow>
      <entry>Ażurowe kamienne ogrodzenie</entry>
      <text>
		         <cell pady="0" width="1000"/>
		         <p/>
		         <cell width="260"/>
		         <cell width="640" bgimg="ui/zoopedia/topCellBg.dds">
		            <cell width="600" padx="20" pady="20">
