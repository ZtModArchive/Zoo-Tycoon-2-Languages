   <!--Zoopedia entries-->
   <zoopedia_candycane>
      <entry>Stoisko z cukrem trzciwnoym</entry>
      <text>
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <p/>
                  <img src="entities\objects\buildings\Snackcart\snackcart_candycane_tt_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>Stoisko z cukrem trzcinowym to wózek wykonany z piernika, cukru i słodyczy. Można w nim kupić smakołyki z cukru trzcinowego.
                  <p/>Słodycze to ulubiony przysmak dzieci, ale ucieszą się także dorośli.
               </color>
            </cell>
         </cell>
      </text>
   </zoopedia_candycane>
   <zoopedia_binoculars_tt>
      <entry>Lornetka w kształcie bałwanka</entry>
      <text>
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <p/>
                  <img src="entities\objects\scenery\binoculars_tt\binoculars_tt_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>Lornetka w kształcie bałwanka to specjalna wersja tego urządzenia, przydatna w trakcie świąt oraz na wybiegach biomu tundry.
                  <p/>Dzięki niej goście będą mogli obejrzeć z bliska zwierzęta, które myślą, że przypatruje im się bałwanek.
               </color>
            </cell>
         </cell>
      </text>
   </zoopedia_binoculars_tt>
   <zoopedia_lowholidayfence_tt>
      <entry>Świąteczny płot</entry>
      <text>
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <img src="entities\objects\fences\lowholidayfence_tt\lowholidayfence_tt_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>Świąteczny płot to standardowe ogrodzenie, najlepiej sprawdzające się w połączeniu z fosą do ogrodzenia wybiegów, lub jako dekoracja alejek.
                  <p/>Ogrodzenie to udekorowane jest lampkami, które fascynują zarówno gości zoo, jak i zwierzęta.
               </color>
            </cell>
         </cell>
      </text>
   </zoopedia_lowholidayfence_tt>
   <zoopedia_reindeer_tt>
      <entry>Podskakujący renifer</entry>
      <text>
         <cell pady="0" width="1000"/>
         <p/>
         <cell width="260"/>
         <cell width="640" height="320" bgimg="ui/zoopedia/topCellBg.dds">
            <cell width="600" padx="20" pady="20">
               <color r="255" g="255" b="255">
                  <p/>
                  <img src="entities\objects\scenery\Playground_BouncyRide_Reindeer_tt/BouncyRide_Reindeer_icon.dds" sx="0" sy="0" sw="64" sh="64"/>
                  <p/>Dzieci lubią bawić się na podskakujących zabawkach. Ten renifer niestety nie lata, ale dostarcza wiele radości najmłodszym gościom zoo.
               </color>
            </cell>
         </cell>
      </text>
   </zoopedia_reindeer_tt>
   <zoopedia_presentapple>
      <entry>Prezent świąteczny (jabłko)</entry>
      <text>
         <cell pady="0" width="1000"/>
